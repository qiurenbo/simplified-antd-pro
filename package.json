    "react": "17.x",
    "react-dev-inspector": "^1.6.0",
    "react-dom": "17.x",
    "umi": "^3.5.13",
    "umi-plugin-antd-icon-config": "^2.0.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
